#include <stdio.h>

int main() {
    int i,j,n;
    printf("enter the value of n:");
    scanf("%d",&n);
    for(i=1;i<=n;i++){
        for(j=1;j<=n;j++){
            if(i%2==0)
            {
            printf("%c",64+i);
            }
            else
            {
            printf("%c",96+i);
            }
        }
        printf("\n");
    }
    return 0;
}

output:
enter the value of n:5
aaaaa
BBBBB
ccccc
DDDDD
eeeee
